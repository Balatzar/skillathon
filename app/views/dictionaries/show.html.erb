<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @dictionary.name %>
</p>

<p>
  <strong>Words:</strong>
  <ul>
    <% @dictionary.words.each do |word| %>
      <li><%= word.name %></li>
    <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_dictionary_path(@dictionary) %> |
<%= link_to 'Back', dictionaries_path %>
